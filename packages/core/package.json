{
  "name": "@osbjs/core",
  "version": "3.0.0-alpha",
  "description": "Render storyboard in js",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build-all": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "build-dts": "rollup --config rollup.config.dts.ts --configPlugin @rollup/plugin-typescript",
    "build": "npm run build-all && npm run build-dts && rimraf dist/types",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@total-typescript/tsconfig": "^1.0.4",
    "rimraf": "^6.0.1",
    "rollup": "^4.19.1",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  }
}
